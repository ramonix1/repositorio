import math
a=int(input('digite o valor de a:'))
if a==0:
    print('o valor não pode ser 0')
    exit()
b=int(input('digite o valor de b:'))
c=int(input('digite o valor de c:'))
delta=b**2 - 4*a*c
if delta>0:
    print('o valor do delta é:', delta)
elif delta==0:
    print('delta ficou zerado. A equação terá apenas uma raíz.')
else:
    print('A equação não terá raízes reais')
    exit()
x1 = (-b + math.pow(delta, 1 / 2)) / (2 * a)
x2 = (-b - math.pow(delta, 1 / 2)) / (2 * a)
print(x1,x2)
